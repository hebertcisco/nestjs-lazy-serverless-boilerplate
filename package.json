{
    "name": "nestjs-lazy-serverless-boilerplate",
    "version": "0.0.1",
    "description": "Î» Nest.js (lazy modules) Serverless boilerplate",
    "author": "Hebert Barros <hebertcisco@outlook.com>",
    "private": true,
    "license": "UNLICENSED",
    "main": "dist/main.js",
    "type": "module",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\"",
        "lint": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
        "preversion": "npm run lint",
        "version": "npm run format && git add -A src",
        "postversion": "git push && git push --tags",
        "dev": "serverless offline start",
        "predev": "npm run build",
        "deploy": "serverless deploy --verbose",
        "start:docs": "node dist/src/swagger.js",
        "prestart:docs": "npm run build"
    },
    "dependencies": {
        "@nestjs/common": "9.1.6",
        "@nestjs/core": "9.0.11",
        "@nestjs/mapped-types": "*",
        "@nestjs/platform-express": "9.1.4",
        "@nestjs/swagger": "^6.1.3",
        "@vendia/serverless-express": "^4.10.1",
        "aws-lambda": "^1.0.7",
        "class-validator": "0.13.2",
        "compression": "1.7.4",
        "cors": "2.8.5",
        "dotenv": "16.0.3",
        "express": "4.18.1",
        "http-status": "1.5.3",
        "nest-shared": "0.4.53",
        "reflect-metadata": "0.1.13",
        "rxjs": "7.5.7",
        "swagger-ui-express": "4.5.0",
        "tsyringe": "4.7.0"
    },
    "devDependencies": {
        "rimraf": "3.0.2",
        "@babel/cli": "7.19.3",
        "@babel/core": "7.19.1",
        "@babel/node": "7.19.1",
        "@babel/preset-env": "7.20.2",
        "@nestjs/cli": "9.1.4",
        "@nestjs/schematics": "9.0.3",
        "@serverless/typescript": "^3.21.0",
        "@types/aws-lambda": "^8.10.106",
        "@types/chance": "1.1.3",
        "@types/cors": "2.8.12",
        "@types/dotenv": "8.2.0",
        "@types/express": "4.17.14",
        "@types/jest": "29.2.0",
        "@types/node": "16.11.7",
        "@typescript-eslint/eslint-plugin": "5.37.0",
        "@typescript-eslint/parser": "5.40.0",
        "@typescript-eslint/typescript-estree": "5.37.0",
        "eslint": "8.26.0",
        "eslint-config-airbnb-base": "15.0.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-prettier": "4.2.1",
        "jest": "29.2.2",
        "nodemon": "^2.0.20",
        "prettier": "2.7.1",
        "serverless-esbuild": "^1.33.0",
        "serverless-offline": "^11.2.3",
        "ts-jest": "29.0.1",
        "ts-loader": "9.4.1",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "4.1.0",
        "typescript": "4.8.3"
    }
}